{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
      "@store/*": [
        "src/store/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}